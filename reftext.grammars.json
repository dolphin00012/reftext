{"scopeName": "source.reftext", "foldingStartMarker": "^#", "patterns": [
{"name": "headline.reftext",    "match": "^#.*$"},
{"name": "dotword.reftext",     "match": "^\\w+-{0,2}(\\w+)?\\.$|^  \\w+-?\\w+\\. |^\\* |^\\*"},
{"name": "asteriskword.reftext","match": "\\*\\w+-?\\w+\\*"},
{"name": "comment.reftext",     "match": "^//.*$"},
{"name": "IN.reftext",          "begin": "\\{", "end": "\\}", "patterns": [ 
{"name": "year.reftext",        "match": "\\d{4}"},
{"name": "comma.reftext",       "match": ",\\s"},
{"name": "else.in.reftext",     "match": "."}
]}]}